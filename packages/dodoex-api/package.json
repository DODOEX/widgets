{
  "name": "@dodoex/api",
  "version": "3.0.0-beta.4",
  "description": "DODO API Kit",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "main": "dist/cjs/index.cjs",
  "module": "dist/index.js",
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/DODOEX/widgets#readme",
  "repository": "https://github.com/DODOEX/widgets",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "yarn run rollup -c --watch",
    "build": "rollup --config",
    "schema:compile": "export $(cat .env | xargs) && env graphql-codegen",
    "schema:watch": "export $(cat .env | xargs) && env graphql-codegen -w",
    "prerelease": "yarn run build",
    "release": "semantic-release",
    "release:local": "np --any-branch --no-cleanup --no-tests"
  },
  "author": "",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "keywords": [
    "dodo",
    "widgets",
    "interfaces"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "lodash": ">=4",
    "bignumber.js": ">=9"
  },
  "dependencies": {
    "cross-fetch": "^4.0.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "qs": "^6.11.2"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.3.2",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.16.7",
    "@ethersproject/abi": ">=5",
    "@ethersproject/contracts": ">=5",
    "@ethersproject/providers": ">=5",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-url": "^8.0.2",
    "@svgr/rollup": "^6.2.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.0.3",
    "babel-plugin-macros": "^3.1.0",
    "babel-runtime": "^6.26.0",
    "dotenv": "^16.0.2",
    "jest": "^29.0.3",
    "jest-environment-hardhat": "^1.1.9",
    "jest-environment-jsdom": "^29.0.3",
    "np": "^7.6.1",
    "rollup": "^4.18.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-dynamic-import-variables": "^1.1.0",
    "svg-jest": "^1.0.1",
    "ts-jest": "^29.0.1",
    "typescript": "^4.7.3"
  }
}