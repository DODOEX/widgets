{
  "name": "@dodoex/icons",
  "version": "2.0.1",
  "description": "DODO icons distributed as SVG React components.",
  "main": "build/index.js",
  "source": "src/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "shx rm -rf build && shx cp -rf lib/ build/ && yarn typescript",
    "build:all": "yarn src:icons && yarn build",
    "build:lib": "yarn build:node && yarn build:stable",
    "build:lib:clean": "rimraf lib/ && yarn build:lib",
    "build:node": "node scripts/build node --largeFiles --outDir lib",
    "build:stable": "node scripts/build stable --largeFiles --outDir lib",
    "typescript": "ts-node scripts/create-typings.ts",
    "src:icons": "cross-env UV_THREADPOOL_SIZE=64 ts-node scripts/builder.ts --output-dir src --svg-dir assets && yarn build:lib:clean",
    "prerelease": "yarn run build",
    "release": "np --any-branch --no-cleanup --no-tests --no-release-draft"
  },
  "author": "",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "dodo",
    "ui",
    "components"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "license": "GPL-3.0-or-later",
  "peerDependencies": {
    "@types/react": "^17.0.0 || ^18.0.0",
    "react": "^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/node": "^7.16.8",
    "babel-plugin-module-resolver": "^4.1.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-transform-object-assign": "^7.16.7",
    "@babel/plugin-transform-react-constant-elements": "^7.17.6",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/register": "^7.17.0",
    "@types/fs-extra": "^9.0.13",
    "@types/mustache": "^4.2.1",
    "@types/rimraf": "^3.0.2",
    "@types/svgo": "^2.6.3",
    "cross-env": "^7.0.3",
    "fs-extra": "^10.1.0",
    "mustache": "^4.2.0",
    "shx": "^0.3.4",
    "svgo": "^2.8.0",
    "ts-node": "^10.8.1",
    "yargs": "^17.5.1"
  }
}