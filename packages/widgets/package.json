{
  "name": "dodo-widgets-inner",
  "version": "1.0.9",
  "description": "DODO Widgets",
  "source": "src/index.tsx",
  "types": "dist/src/index.d.ts",
  "main": "dist/cjs/index.cjs",
  "module": "dist/index.js",
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/shortcutBoy/widgets-beta#readme",
  "repository": "https://github.com/shortcutBoy/widgets-beta",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "yarn compile:i18n",
    "extract:i18": "lingui extract --locale en-US",
    "compile:i18n": "yarn extract:i18 && lingui compile",
    "watch": "yarn run rollup -c --watch",
    "prebuild": "yarn workspace @dodoex-io/icons build && yarn workspace @dodoex-io/components build",
    "build": "yarn prebuild && yarn compile:i18n && rollup --config",
    "prerelease": "yarn build",
    "release": "semantic-release",
    "test": "jest"
  },
  "author": "",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "keywords": [
    "dodo",
    "widgets",
    "interfaces"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.17.0",
    "@dodoex-io/components": "^1.0.6",
    "@dodoex-io/icons": "^1.0.1",
