    "babel-plugin-macros": "^3.1.0",
    "babel-runtime": "^6.26.0",
    "dotenv": "^16.0.2",
    "ethers": "^5.7.1",
    "hardhat": "^2.11.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.3",
