    "react": ">=17.0.1",
    "react-dom": ">=17.0.1",
    "react-redux": ">=7.2.2",
    "redux": ">=4.1.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.16.7",
    "@lingui/babel-preset-react": "^2.9.2",
    "@lingui/cli": "^3.14.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomiclabs/hardhat-ethers": "^2.1.1",
